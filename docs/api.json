    "definitions": {
        "Headers": {
            "type": "object",
            "required": [
                "POS-NAME",
                "POS-VERSION"
            ],
            "properties": {
                "POS-NAME": {
                    "type": "string",
                    "description": "Name of your Point of Sales",
                    "example": "Assembly POS"
                },                                        
                "POS-VERSION": {
                    "type": "string",
                    "description": "Version of your Point of Sales",
                    "example": "10.2.99"
                }
            }                           
        },
        "Purchase": {
            "type": "object",
            "required": [
                "posRefId",
                "purchaseAmount"
            ],
            "properties": {
                "posRefId": {
                    "type": "string",
                    "description": "Unique alphanumeric identifier",
                    "example": "purchaseTrans1"
                },
                "purchaseAmount": {
                    "type": "integer",
                    "format": "int32",
                    "description": "Total amount of the transaction that was performed to the bank in cents",
                    "example": 10000
                },
                "tipAmount": {
                    "type": "integer",
                    "format": "int32",
                    "description": "Tip component of a transaction in cents",
                    "example": 200
                },
                "cashoutAmount": {
                    "type": "integer",
                    "format": "int32",
                    "description": "Amount to cashout in cents",
                    "example": 0
                }
            }
        },
        "PurchaseQueryParam": {
            "type": "object",
            "required": [
                "posRefId",
                "purchaseAmount"
            ],
            "properties": {
                "posRefId": {
                    "type": "string",
                    "description": "posRefId of your last purchase transaction",
                    "example": "purchaseTrans1"
                }
            }
        },
        "Refund": {
            "type": "object",
            "required": [
                "posRefId",
                "refundAmount"
            ],
            "properties": {
                "posRefId": {
                    "type": "string",
                    "description": "Unique alphanumeric identifier",
                    "example": "refundtrans1"
                },
                "refundAmount": {
                    "type": "integer",
                    "description": "Total amount of the transaction that was performed to the bank in cents",
                    "example": 5000
                }
            }
        },
        "SettlementEnquiry": {
            "type": "object",
            "required": [
                "posRefId"
            ],
            "properties": {
                "posRefId": {
                    "type": "string",
                    "description": "Unique alphanumeric identifier",
                    "format": "settlementEnq1"
                }
            }
        },        
        "Settlement": {
            "type": "object",
            "required": [
                "posRefId",
                "refundAmount"
            ],
            "properties": {
                "posRefId": {
                    "type": "string",
                    "description": "Unique alphanumeric identifier",
                    "format": "settlement"
                }
            }
        },        
